{
  "name": "indexer",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build sepolia && nest build mainnet",
    "build:mainnet": "nest build mainnet",
    "build:sepolia": "nest build sepolia",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "start:dev:sepolia": "ENV_FILE=.env.sepolia nest start --watch",
    "start:dev:mainnet": "ENV_FILE=.env.mainnet nest start --watch",
    "engage": "yarn build && pm2 start pm2.config.js && pm2 log",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "copy:assets": "cp -rv ./src/temp ./dist/temp",
    "test": "jest"
  },
