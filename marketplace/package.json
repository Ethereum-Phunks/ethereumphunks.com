{
  "name": "etherphunks-market",
  "version": "2.1.0",
  "scripts": {
    "prebuild": "node prebuild.js",
    "ng": "ng",
    "start:sepolia": "ng serve --configuration dev-sepolia --host 0.0.0.0 --port 4200",
    "start:mainnet": "ng serve --configuration dev-mainnet --host 0.0.0.0 --port 4200",
    "build": "yarn build:sepolia && yarn build:mainnet",
    "build:sepolia": "yarn prebuild && ng build --configuration sepolia",
    "build:mainnet": "yarn prebuild && ng build --configuration mainnet",
    "test": "ng test",
    "run-built": "http-server dist/etherphunks-market -p 8000",
    "translate": "ng extract-i18n --output-path src/locale",
    "translate:json": "ng extract-i18n --output-path src/locale --format=json"
  },
