{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "etherphunks-market": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-esbuild:application",
          "options": {
            "optimization": true,
            "outputPath": {
              "base": "dist/etherphunks-market"
            },
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/_redirects",
              "src/manifest.webmanifest"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/scss"
              ]
            },
            "styles": [
              "./node_modules/@ng-select/ng-select/scss/default.theme.scss",
              "src/scss/main.scss"
            ],
            "scripts": [],
            "serviceWorker": "ngsw-config.json",
            "webWorkerTsConfig": "tsconfig.worker.json",
            "allowedCommonJsDependencies": [
              "@xmtp/xmtp-js",
              "@xmtp/proto",
              "protobufjs/minimal",
              "protobufjs"
            ],
            "preserveSymlinks": true,
            "browser": "src/main.ts"
          },
          "configurations": {
            "dev-sepolia": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev-sepolia.ts"
                }
              ]
            },
            "dev-mainnet": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev-mainnet.ts"
                }
              ]
            },
            "sepolia": {
              "outputPath": {
                "base": "dist/etherphunks-market-sepolia_may18"
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "50mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.sepolia.ts"
                }
              ],
              "index": {
                "input": "src/index.sepolia.html",
                "output": "index.html"
              },
              "outputHashing": "all"
            },
            "mainnet": {
              "outputPath": {
                "base": "dist/etherphunks-market-mainnet_may18"
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "50mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.mainnet.ts"
                }
              ],
              "index": {
                "input": "src/index.mainnet.html",
                "output": "index.html"
              },
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-esbuild:dev-server",
          "configurations": {
            "dev-sepolia": {
              "buildTarget": "etherphunks-market:build:dev-sepolia"
            },
            "dev-mainnet": {
              "buildTarget": "etherphunks-market:build:dev-mainnet"
            },
            "sepolia": {
              "buildTarget": "etherphunks-market:build:sepolia"
            },
            "mainnet": {
              "buildTarget": "etherphunks-market:build:mainnet"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "etherphunks-market:build"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}